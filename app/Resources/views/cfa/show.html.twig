{% extends 'base.html.twig' %}

{% block body %}
    <h1>Cfa</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cFA.id }}</td>
            </tr>
            <tr>
                <th>Textpresident</th>
                <td>{{ cFA.textPresident }}</td>
            </tr>
            <tr>
                <th>Historical</th>
                <td>{{ cFA.historical }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('cfa_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('cfa_edit', { 'id': cFA.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
